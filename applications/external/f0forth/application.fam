App(
    appid="f0forth",
    name="F0 Forth",
    apptype=FlipperAppType.EXTERNAL,
    entry_point="f0forth",
    stack_size=16 * 1024,
    fap_private_libs=[
        Lib(
            name="pforth",
            cflags=[
                "-DPF_USER_CUSTOM",
                "-DPF_NO_INIT",
                '-DPF_USER_FILEIO=\\"../../pf_fileio_f0.h\\"',
                '-DPF_USER_INC1=\\"../../pf_malloc_f0.h\\"',
                "-DPF_NO_CLIB",
                "-DPF_STATIC_DIC",
                "-Wno-redundant-decls",
                "-Wno-cast-function-type",
                "-Wno-unused-parameter",
                "-Wno-sign-compare",
                "-Wno-incompatible-pointer-types",
                "-Wno-shift-count-overflow",
            ],
        ),
    ],
    fap_icon="forth10px.png",
    fap_category="Tools",
    fap_author="twisted-pear",
    fap_weburl="https://github.com/twisted-pear/f0forth",
    fap_version=(1, 0),
    fap_description="Once started the plugin will add a forth command to the CLI that can be used to call up a basic forth shell.",
)
