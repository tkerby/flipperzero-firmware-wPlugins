App(
    appid="nfc",
    name="NFC",
    apptype=FlipperAppType.MENUEXTERNAL,
    targets=["f7"],
    entry_point="nfc_app",
    icon="A_NFC_14",
    stack_size=5 * 1024,
    order=30,
    resources="resources",
    sources=[
        "*.c*",
        "!plugins",
        "!nfc_cli.c",
    ],
    fap_libs=["mbedtls"],
    fap_icon="icon.png",
    fap_icon_assets="images",
    fap_category="NFC",
)

